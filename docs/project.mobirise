ondition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"container align-center\">\n        <div class=\"row justify-content-md-center\">\n            <div class=\"mbr-white col-md-10\">\n                <h1 class=\"mbr-section-title mbr-bold pb-3 mbr-fonts-style\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\"><span style=\"color:#E07E01\">asp</span><span style=\"color: rgb(20, 157, 204); font-weight: normal;letter-spacing:-0.12em\"><i>Lite</i></span></h1>\n              \n                <p class=\"mbr-text pb-3 mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\">a future for ASP/VBScript</p>\n                <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\"><a class=\"btn btn-md btn-primary\" href=\"index.html#about\" data-app-placeholder=\"Type Text\"><span class=\"fa fa-navicon mbr-iconfont mbr-iconfont-btn\"></span>read more</a> <a class=\"btn btn-md btn-secondary\" href=\"https://github.com/aspLite/aspLite\" data-app-placeholder=\"Type Text\" target=\"_blank\"><span class=\"fa fa-github mbr-iconfont mbr-iconfont-btn\"></span>download</a> <a class=\"btn btn-md btn-info\" href=\"https://demo.asplite.com/ebook.asp\" data-app-placeholder=\"Type Text\" target=\"_blank\"><span class=\"icon54-v3-book-2 mbr-iconfont mbr-iconfont-btn\"></span>handbook</a></div>\n            </div>\n        </div>\n    </div>\n    <div mbr-if=\"showArrow\" class=\"mbr-arrow hidden-sm-down\" aria-hidden=\"true\">\n        <a href=\"#next\">\n            <i class=\"mbri-down mbr-iconfont\"></i>\n        </a>\n    </div>\n</section>",
